<div class="addHour">
<%= form_for @officehour do |f| %> 
  <p><%= label_tag 'Start Time' %><br/>
  <%= f.text_field :start_time %></p>
  
  <p><%= label_tag 'End Time' %><br/>
  <%= f.text_field :end_time %></p>
  
  <p><%= label_tag 'Location' %><br/>
  <%= f.text_field :location %></p>
  
  <p><%= label_tag 'Location details' %><br/>
  <%= f.text_field :location_details %></p>

  <p><%= submit_tag 'Add office hour' %></p>
  <% end %>
</div>
